{
  "name": "nuxt-kirby",
  "type": "module",
  "version": "3.0.2",
  "packageManager": "pnpm@10.28.1",
  "description": "Kirby CMS integration for Nuxt with KQL support",
  "author": "Johann Schopplich <hello@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://nuxt-kirby.byjohann.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/nuxt-kirby.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/nuxt-kirby/issues"
  },
  "keywords": [
    "getkirby",
    "kirby",
    "kql",
    "nuxt",
    "query"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "default": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxt dev playground",
    "dev:build": "nuxt build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare playground && nuxt prepare test/fixture",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:types": "tsc --noEmit",
    "release": "bumpp"
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.2",
    "@vueuse/core": "^14.1.0",
    "defu": "^6.1.4",
    "kirby-types": "^1.4.6",
    "ofetch": "^1.5.1",
    "ohash": "^2.0.11",
    "pathe": "^2.0.3",
    "scule": "^1.3.0",
    "ufo": "^1.6.3",
    "uint8array-extras": "^1.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.1.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/test-utils": "^3.23.0",
    "@types/node": "^24.10.9",
    "bumpp": "^10.4.0",
    "destr": "^2.0.5",
    "eslint": "^9.39.2",
    "nuxt": "^4.2.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
